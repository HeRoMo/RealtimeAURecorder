{
  "name": "realtime-au-recorder",
  "version": "2.0.0",
  "description": "A Google Apps Script application to record realtime active users from Google Analytics.",
  "main": "index.js",
  "scripts": {
    "login": "./node_modules/.bin/clasp login",
    "init_gas": "./node_modules/.bin/clasp create RealtimeAuRecorder",
    "postinit_gas": "node ./utils/modify_clasp_json.js && rm ./appsscript.json",
    "deploy": "./node_modules/.bin/clasp push"
  },
  "keywords": [
    "gas",
    "analytics",
    "google sheets"
  ],
  "author": "HeRoMo",
  "repository": "https://github.com/HeRoMo/RealtimeAURecorder",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@google/clasp": "^2.3.0",
    "@types/google-apps-script": "^1.0.14"
  }
}
