{
  "name": "RealtimeAURecorder",
  "version": "1.0.0",
  "description": "A Google Apps Script application to record realtime active users from Google Analytics.",
  "main": "index.js",
  "scripts": {
    "login": "./node_modules/.bin/clasp login",
    "init_gas": "./node_modules/.bin/clasp create RealtimeAuRecorder",
    "postinit_gas": "node ./utils/modify_clasp_json.js && rm ./appsscript.json",
    "build": "webpack",
    "cleanup": "rm -rf dest/*",
    "deploy": "./node_modules/.bin/clasp push",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc -p .",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run check"
  },
  "keywords": [
    "gas",
    "analytics",
    "google sheets"
  ],
  "author": "HeRoMo",
  "repository": "https://github.com/HeRoMo/RealtimeAURecorder",
  "license": "MIT",
  "dependencies": {
    "@types/google-apps-script": "0.0.28"
  },
  "devDependencies": {
    "@google/clasp": "^1.5.3",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-gas": "^1.0.0",
    "copy-webpack-plugin": "^4.5.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-googleappsscript": "^1.0.0",
    "eslint-plugin-import": "^2.9.0",
    "gas-webpack-plugin": "^0.2.1",
    "gts": "^0.8.0",
    "typescript": "^2.9.2",
    "webpack": "^3.11.0"
  }
}
